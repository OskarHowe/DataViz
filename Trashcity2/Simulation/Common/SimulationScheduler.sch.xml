<csmSon:SchedulerInstance xmlns:csmSon="http://www.cosmotech.com/csm_scheduler/8.4">
  <csmSon:RepeatTaskBlock repetitions="52">

    <csmSon:RepeatTaskBlock repetitions="7">
      <csmSon:SequentialTaskBlock>

        <csmSon:ForEachTask recursiveLevel="2" concurrencyLevel="8">
          <csmSon:ModelName>Model</csmSon:ModelName>
          <csmSon:Entity>Lyon</csmSon:Entity>
          <csmSon:SubEntityType>Inhabitant</csmSon:SubEntityType>
          <csmSon:Process>ProduceTrash</csmSon:Process>
        </csmSon:ForEachTask>

        <csmSon:ForEachTask recursiveLevel="2" concurrencyLevel="8">
          <csmSon:ModelName>Model</csmSon:ModelName>
          <csmSon:Entity>Lyon</csmSon:Entity>
          <csmSon:SubEntityType>Inhabitant</csmSon:SubEntityType>
          <csmSon:Process>UpdateSatisfaction</csmSon:Process>
        </csmSon:ForEachTask>

        <csmSon:ForEachTask recursiveLevel="1" concurrencyLevel="1">
          <csmSon:ModelName>Model</csmSon:ModelName>
          <csmSon:Entity>Lyon</csmSon:Entity>
          <csmSon:SubEntityType>Street</csmSon:SubEntityType>
          <csmSon:Process>MesoPerRct</csmSon:Process>
        </csmSon:ForEachTask>

      </csmSon:SequentialTaskBlock>
    </csmSon:RepeatTaskBlock>

    <csmSon:SequentialTaskBlock>

      <csmSon:ForEachTask recursiveLevel="1" concurrencyLevel="1">
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:SubEntityType>Street</csmSon:SubEntityType>
        <csmSon:MesoFunction>OverviewStreet</csmSon:MesoFunction>
      </csmSon:ForEachTask>

      <csmSon:ForEachTask recursiveLevel="1" concurrencyLevel="1">
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:SubEntityType>Street</csmSon:SubEntityType>
        <csmSon:Process>CalcTrucksNeededAndActivate</csmSon:Process>
      </csmSon:ForEachTask>

      <csmSon:ForEachTask recursiveLevel="2" concurrencyLevel="8">
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:SubEntityType>Truck</csmSon:SubEntityType>
        <csmSon:Process>CalcCost</csmSon:Process>
      </csmSon:ForEachTask>

      <csmSon:ForEachTask recursiveLevel="1" concurrencyLevel="1">
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:SubEntityType>Street</csmSon:SubEntityType>
        <csmSon:Process>MesoPickupTrash</csmSon:Process>
      </csmSon:ForEachTask>

      <csmSon:ForEachTask recursiveLevel="2" concurrencyLevel="8">
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:SubEntityType>Truck</csmSon:SubEntityType>
        <csmSon:Process>EmptyLoad</csmSon:Process>
      </csmSon:ForEachTask>

      <csmSon:ForEachTask recursiveLevel="1" concurrencyLevel="4">
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:SubEntityType>Street</csmSon:SubEntityType>
        <csmSon:Process>CalcTruckCostTotal</csmSon:Process>
      </csmSon:ForEachTask>
      <csmSon:ForEachTask>
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:SubEntityType>Street</csmSon:SubEntityType>
        <csmSon:Process>CalManif</csmSon:Process>
      </csmSon:ForEachTask>
      <csmSon:ModelTask>Model</csmSon:ModelTask>
      <csmSon:SimpleTask>
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Entity>Lyon</csmSon:Entity>
        <csmSon:MesoFunction>Overview</csmSon:MesoFunction>
      </csmSon:SimpleTask>
    </csmSon:SequentialTaskBlock>
  </csmSon:RepeatTaskBlock>
</csmSon:SchedulerInstance>
