<csmSon:SchedulerInstance xmlns:csmSon="http://www.cosmotech.com/csm_scheduler/8.4">
  <csmSon:SequentialTaskBlock>
    <csmSon:EnvironmentTask>
      <csmSon:ModelName>Model</csmSon:ModelName>
      <csmSon:Process>Initialize</csmSon:Process>
    </csmSon:EnvironmentTask>
    <csmSon:ForEachTask>
      <csmSon:ModelName>Model</csmSon:ModelName>
      <csmSon:Entity>IndustrialNetwork</csmSon:Entity>
      <csmSon:SubEntityType>Stock</csmSon:SubEntityType>
      <csmSon:Process>Initialize</csmSon:Process>
    </csmSon:ForEachTask>
    <csmSon:WhileTaskBlock>
      <csmSon:Condition>
        <csmSon:Repetitions value="{Model}Model::{Attribute}ActivateUncertainties"/>
      </csmSon:Condition>
      <csmSon:SequentialTaskBlock>
        <csmSon:ForEachTask>
          <csmSon:ModelName>Model</csmSon:ModelName>
          <csmSon:Entity>IndustrialNetwork</csmSon:Entity>
          <csmSon:SubEntityType>Stock</csmSon:SubEntityType>
          <csmSon:Process>VariateDemands</csmSon:Process>
        </csmSon:ForEachTask>
      </csmSon:SequentialTaskBlock>
    </csmSon:WhileTaskBlock>
  </csmSon:SequentialTaskBlock>
  <csmSon:WhileTaskBlock>
    <csmSon:Condition>
      <csmSon:Repetitions value="{Model}Model::{Attribute}NumberOfCycle"/>
    </csmSon:Condition>
    <csmSon:SequentialTaskBlock>
      <csmSon:WhileTaskBlock>
        <csmSon:Condition>
          <csmSon:Repetitions value="{Model}Model::{Attribute}TimeStepPerCycle"/>
        </csmSon:Condition>
        <csmSon:SequentialTaskBlock>
          <csmSon:ForEachTask>
            <csmSon:ModelName>Model</csmSon:ModelName>
            <csmSon:Entity>IndustrialNetwork</csmSon:Entity>
            <csmSon:SubEntityType>Stock</csmSon:SubEntityType>
            <csmSon:Process>Update</csmSon:Process>
          </csmSon:ForEachTask>
          <csmSon:ForEachTask>
            <csmSon:ModelName>Model</csmSon:ModelName>
            <csmSon:Entity>IndustrialNetwork</csmSon:Entity>
            <csmSon:SubEntityType>Operation</csmSon:SubEntityType>
            <csmSon:Process>Update</csmSon:Process>
          </csmSon:ForEachTask>
          <csmSon:ForEachTask>
            <csmSon:ModelName>Model</csmSon:ModelName>
            <csmSon:Entity>IndustrialNetwork</csmSon:Entity>
            <csmSon:SubEntityType>Machine</csmSon:SubEntityType>
            <csmSon:Process>Update</csmSon:Process>
          </csmSon:ForEachTask>
          <csmSon:WhileTaskBlock>
            <csmSon:Condition>
              <csmSon:Repetitions value="{Model}Model::{Attribute}ActivateUncertainties"/>
            </csmSon:Condition>
            <csmSon:SequentialTaskBlock>
              <csmSon:ForEachTask>
                <csmSon:ModelName>Model</csmSon:ModelName>
                <csmSon:Entity>IndustrialNetwork</csmSon:Entity>
                <csmSon:SubEntityType>Operation</csmSon:SubEntityType>
                <csmSon:Process>VariateOperatingPerformance</csmSon:Process>
              </csmSon:ForEachTask>
            </csmSon:SequentialTaskBlock>
          </csmSon:WhileTaskBlock>
          <csmSon:ModelTask>Model</csmSon:ModelTask>
          <csmSon:ForEachTask>
            <csmSon:ModelName>Model</csmSon:ModelName>
            <csmSon:Entity>IndustrialNetwork</csmSon:Entity>
            <csmSon:SubEntityType>Stock</csmSon:SubEntityType>
            <csmSon:Process>CumulateCosts</csmSon:Process>
          </csmSon:ForEachTask>
          <csmSon:EnvironmentTask>
            <csmSon:ModelName>Model</csmSon:ModelName>
            <csmSon:Process>IncrementTimeStep</csmSon:Process>
          </csmSon:EnvironmentTask>
        </csmSon:SequentialTaskBlock>
      </csmSon:WhileTaskBlock>
      <csmSon:EnvironmentTask>
        <csmSon:ModelName>Model</csmSon:ModelName>
        <csmSon:Process>IncrementCycle</csmSon:Process>
      </csmSon:EnvironmentTask>
    </csmSon:SequentialTaskBlock>
  </csmSon:WhileTaskBlock>
</csmSon:SchedulerInstance>
