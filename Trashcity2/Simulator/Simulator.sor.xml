<csmSor:Simulator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:csmSor="http://www.cosmotech.com/csm_simulator/8.8">
  <csmSor:Name>Trashcity2</csmSor:Name>
  <csmSor:Version><![CDATA[0.1.0]]></csmSor:Version>
  <csmSor:Description><![CDATA[]]></csmSor:Description>
  <csmSor:Vendor><![CDATA[Cosmo Tech]]></csmSor:Vendor>
  <csmSor:License><![CDATA[]]></csmSor:License>
  <csmSor:ConceptualModels>
    <csmSor:ModelReference id="Trashcity2" alias="Trashcity2">
      <csmSor:Resource>Trashcity2</csmSor:Resource>
    </csmSor:ModelReference>
  </csmSor:ConceptualModels>
  <csmSor:InlineInstrumentation>
    <csmSor:Probes>
      <csmSor:Probe xsi:type="csmSor:StarProbe" name="Trucks">
        <csmSor:Observables>
          <csmSor:EntityReference name="TruckObs">{Model}Trashcity2::{Entity}Truck</csmSor:EntityReference>
        </csmSor:Observables>
        <csmSor:Measure xsi:type="csmSor:DataType" name="Fullness" updateRate="OncePerObservablesValue" owned="1">
          <csmSor:DataTypeReference>{Model}Trashcity2::{DataType}Integer</csmSor:DataTypeReference>
          <csmSor:CPPCode>GetFullness() = GetTruckObs()->GetFullness();</csmSor:CPPCode>
        </csmSor:Measure>
        <csmSor:Dimension xsi:type="csmSor:DataType" name="TruckName" updateRate="OncePerObservablesValue" owned="1">
          <csmSor:DataTypeReference>{Model}Trashcity2::{DataType}String</csmSor:DataTypeReference>
          <csmSor:CPPCode>GetTruckName() = GetTruckObs()->GetName();</csmSor:CPPCode>
        </csmSor:Dimension>
      </csmSor:Probe>

      <csmSor:Probe xsi:type="csmSor:StarProbe" name="Street">
        <csmSor:Observables>
          <csmSor:EntityReference name="StreetObs">{Model}Trashcity2::{Entity}Street</csmSor:EntityReference>
        </csmSor:Observables>
        <csmSor:Measure xsi:type="csmSor:DataType" name="TotalTrash" updateRate="OncePerObservablesValue" owned="1">
          <csmSor:DataTypeReference>{Model}Trashcity2::{DataType}Integer</csmSor:DataTypeReference>
          <csmSor:CPPCode>GetTotalTrash() = GetStreetObs()->GetTotalTrash();</csmSor:CPPCode>
        </csmSor:Measure>
        
      </csmSor:Probe>
    </csmSor:Probes>
  </csmSor:InlineInstrumentation>
  <csmSor:Plugins/>
</csmSor:Simulator>

